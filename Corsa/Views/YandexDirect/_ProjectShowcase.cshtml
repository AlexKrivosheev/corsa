@model Corsa.Models.Moduls.ProjectModuleViewModel
<div class="modul-selector-item col-lg-3 col-md-6 col-sm-12 mb-4 " onclick='location.href="@Url.Action(Model.ViewDescriptor.OpenAction,Model.ViewDescriptor.Controller,new { id = Model.Module.Id })"'>
    <div class="crs-yandex-direct-showcase card">
        <div class="card-body">
            <p class="project-module-identity"> <i class="crs-yandex-direct-showcase__icon fab fa-yandex-international"></i> @Model.ViewDescriptor.Name </p>
            <h5 class="crs-yandex-direct-showcase__title card-title">@Model.Module.Name</h5>
            <p class="crs-yandex-direct-showcase__desc card-text">@Model.Module.Description</p>

            <div class="crs-yandex-direct-showcase__action bar-actions">
                <ul class="nav justify-content-end">
                    <li class="nav-item action ml-3">
                        <a href="@Url.Action("Edit","YandexDirect",new {Id=Model.Module.Id})"><i class="fas fa-pencil-alt"></i></a>
                    </li>
                    <li class="nav-item action ml-3">
                        <a href="@Url.Action("Delete","YandexDirect",new {Id=Model.Module.Id})"><i class="fas fa-trash"></i></a>
                    </li>

                </ul>
            </div>
        </div>
    </div>
</div>